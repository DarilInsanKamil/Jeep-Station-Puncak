{
  "name": "jspapp-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "api",
    "fe"
  ],
  "scripts": {
    "dev:api": "bun run --filter api dev",
    "dev:fe": "bun run --filter fe dev",
    "dev:all": "bun run --filter '*' dev",
    "build:all": "bun run --filter '*' build"
  },
  "dependencies": {
    "@types/bun": "^1.3.8"
  }
}